BEGIN TRANSACTION;
GO

DECLARE @var0 sysname;
SELECT @var0 = [d].[name]
FROM [sys].[default_constraints] [d]
INNER JOIN [sys].[columns] [c] ON [d].[parent_column_id] = [c].[column_id] AND [d].[parent_object_id] = [c].[object_id]
WHERE ([d].[parent_object_id] = OBJECT_ID(N'[ST_MATERIALS]') AND [c].[name] = N'Quantity');
IF @var0 IS NOT NULL EXEC(N'ALTER TABLE [ST_MATERIALS] DROP CONSTRAINT [' + @var0 + '];');
ALTER TABLE [ST_MATERIALS] DROP COLUMN [Quantity];
GO

CREATE TABLE [ST_MATERIALS_STATUS] (
    [Status] int NOT NULL,
    [MaterialId] int NOT NULL,
    [Quantity] real NOT NULL,
    CONSTRAINT [PK_ST_MATERIALS_STATUS] PRIMARY KEY ([MaterialId], [Status]),
    CONSTRAINT [FK_ST_MATERIALS_STATUS_ST_MATERIALS_MaterialId] FOREIGN KEY ([MaterialId]) REFERENCES [ST_MATERIALS] ([Id]) ON DELETE CASCADE
);
GO

UPDATE [ST_AREAS] SET [CreatedAt] = '2024-10-16T21:06:25.9040371Z', [UpdatedAt] = '2024-10-16T21:06:25.9040371Z'
WHERE [Id] = 1;
SELECT @@ROWCOUNT;

GO

